{
    "name": "Cassow for Chrome",
    "version": "1.4.0",
    "description": "Cassow for Chrome.",
    "icons": {
        "16": "img/icon_16.png",
        "48": "img/icon_48.png",
        "128": "img/icon_128.png"
    },
    "manifest_version": 2,
    "browser_action": {
        "default_icon": {
            "19": "img/icon_19.png",
            "38": "img/icon_38.png"
        },
        "default_popup": "html/popup.html"
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Alt+C",
          "mac": "Alt+C",
          "linux": "Alt+C",
          "windows": "Alt+C"
        }
      }
    },
    "omnibox": {"keyword": "c"},
    "options_page": "html/options.html",
    "permissions": [
        "webRequest",
        "*://*.cassow.com/"
    ]
}
